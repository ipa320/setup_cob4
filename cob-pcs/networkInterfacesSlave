# The loopback network interface
auto lo
iface lo inet loopback

# The primary network interface
auto eth0
iface eth0 inet dhcp

# CAN devices
allow-hotplug can0
iface can0 can static
    bitrate 500000

allow-hotplug can1
iface can1 can static
    bitrate 500000

allow-hotplug can2
iface can2 can static
    bitrate 500000

allow-hotplug can3
iface can3 can static
    bitrate 500000
